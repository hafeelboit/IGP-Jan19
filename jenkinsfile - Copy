pipeline {
    agent any

    stages {

        stage('Checkout') {
            steps {
                git 'https://github.com/hafeelboit/IGP-Jan19.git'
            }
        }

        stage('Compile') {
            steps {
                sh 'mvn compile'
            }
        }

        stage('Test') {
            steps {
                sh 'mvn test'
            }
        }

        stage('Package') {
            steps {
                sh 'mvn package'
            }
        }
    }
}
